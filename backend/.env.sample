PORT = "your port number"
JWT_ACCESS_TOKEN = "your jwt access token"
JWT_REFRESH_TOKEN = "your jwt refresh token"
JWT_RESET_TOKEN = "your jwt reset token"
MONGODB_URL = "your mongodb url"
ACCESS_TOKEN_EXPIRES_IN = "access token expiry time"
REFRESH_TOKEN_EXPIRES_IN = "refresh token expiry time"
